<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".PropiedadesInventario">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="347dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/button_rounded"
        android:gravity="center"
        android:text="@string/revisa_el_total_de_tus_propiedades"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="347dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/button_rounded_border"
        android:gravity="center"
        android:text="@string/propiedad_n_1"
        android:textColor="#689F38"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/TTcorte"
        android:layout_width="165dp"
        android:layout_height="26dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="@string/total_de_cosechados"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toStartOf="@+id/CantCos1"
        app:layout_constraintHorizontal_bias="0.006"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TTembolsado" />

    <TextView
        android:id="@+id/TTembolsado"
        android:layout_width="167dp"
        android:layout_height="26dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="@string/total_de_embolsados"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toStartOf="@+id/CantEmb1"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="343dp"
        android:layout_height="38dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/button_rounded_border"
        android:gravity="center"
        android:text="@string/propiedad_n_2"
        android:textColor="#689F38"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TTcorte"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/TTcorte2"
        android:layout_width="163dp"
        android:layout_height="26dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:text="@string/total_de_cosechados2"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/CantCos2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TTembolsado2"
        app:layout_constraintVertical_bias="0.047" />

    <TextView
        android:id="@+id/TTembolsado2"
        android:layout_width="164dp"
        android:layout_height="27dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:text="@string/total_de_embolsados"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toStartOf="@+id/CantEmb2"
        app:layout_constraintHorizontal_bias="0.006"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/CantEmb1"
        android:layout_width="62dp"
        android:layout_height="25dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="20dp"
        android:text=" "
        app:layout_constraintBottom_toTopOf="@+id/CantCos1"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/CantCos1"
        android:layout_width="63dp"
        android:layout_height="25dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="48dp"
        android:text=" "
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/CantEmb2"
        android:layout_width="61dp"
        android:layout_height="20dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="32dp"
        android:text=" "
        app:layout_constraintBottom_toTopOf="@+id/CantCos2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/CantCos2"
        android:layout_width="59dp"
        android:layout_height="21dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="280dp"
        android:text=" "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
